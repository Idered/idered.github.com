---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Kasper Mikiewicz</title>
	<link href="http://idered.pl/feed.xml" rel="self"/>
	<link href="http://idered.pl/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://idered.pl/</id>
	<author>
		<name>Kasper Mikiewicz</name>
		<email>idered@gmail.com</email>
	</author>
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://idered.pl{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://idered.pl{{ post.id }}</id>
		{% if post.meta %}
		<content type="html">{{ post.meta }}</content>
		{% endif %}
	</entry>
	{% endfor %}
</feed>